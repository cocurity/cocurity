    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.0",
    "postcss": "^8.5.3",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2"
  }
