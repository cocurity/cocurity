{
  "name": "launchpass",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "dev:up": "docker compose up -d --wait && npx prisma migrate deploy && next dev",
    "dev:reset": "docker compose up -d --wait && npx prisma migrate reset --force",
    "dev:down": "docker compose down",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.3.1",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@resvg/resvg-js": "^2.6.2",
    "@vercel/blob": "^2.2.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.0",
    "next": "^15.2.0",
    "next-auth": "^5.0.0-beta.30",
    "openai": "^6.22.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-qr-code": "^2.0.18",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.1",
    "three": "^0.182.0"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
